{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "module": "es2020",
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "form-validation": [
        "dist/form-validation/form-validation",
        "dist/form-validation"
      ],
      "validation-lib": [
        "dist/validation-lib/validation-lib",
        "dist/validation-lib"
      ]
    }
  }
}